{
  "name": "electron-react-ts",
  "version": "0.1.0",
  "private": true,
  "main": "./build/main.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.24",
